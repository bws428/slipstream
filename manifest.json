{
  "manifest_version": 3,
  "name": "Slipstream",
  "version": "1.0.6",
  "description": "Streamline your aviation record keeping.",
  "icons": { "128": "icon.png" },
  "host_permissions": [
    "*://workspace.spirit.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://workspace.spirit.com/cvpn/https/ctweb.spirit.com/CrewWeb/ctwpm.aspx*"
      ],
      "css": [
        "node_modules/normalize.css/normalize.css",
        "node_modules/jquery-ui-dist/jquery-ui.min.css",
        "lib/style.css"],
      "js": [
        "node_modules/jquery/dist/jquery.min.js",
        "node_modules/jquery-ui-dist/jquery-ui.min.js",
        "lib/slipstream.js"]
    }
  ]
}
